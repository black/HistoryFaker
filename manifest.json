{
  "manifest_version": 2,
  "name": "Fake History",
  "description": "This extension deletes your browsing history and replaces it with a fake history.",
  "version": "1.0",
  "permissions": [
    "history",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "app/popup.html"
  }
}